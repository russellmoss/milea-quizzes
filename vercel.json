{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": ".",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOWALL"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://*.thinkific.com https://thinkific.com https://learn.thinkific.com"
        }
      ]
    }
  ]
} 