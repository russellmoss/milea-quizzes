{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": ".",
        "buildCommand": "npm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/config.js",
      "headers": {
        "content-type": "application/javascript",
        "cache-control": "no-cache"
      }
    },
    {
      "src": "/dist/(.*)",
      "headers": {
        "content-type": "text/css",
        "cache-control": "public, max-age=31536000"
      }
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 